{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": [
      "app.config",
      "*.yml",
      "*.ps1"
    ]
  },

  "publishOptions": {
    "include": [
      "app.config",
      "*.yml",
      "*.ps1"
    ]
  },

  "dependencies": {
    
    "NServiceBus": "5.2.14",
    "NServiceBus.RabbitMQ": "3.3.3",
    "NServiceBus.Persistence.MongoDb": "6.0.4" 
  },

  "frameworks": {
    "net451": {
      "dependencies": {
        "Microsoft.NETCore.Platforms": "1.0.1",
        "PingMessages": {
          "target": "project"
        },
        "PongMessages": {
          "target": "project"
        },
        "SharedKernel": {
          "target": "project"
        }
      }
    }
  }
}
